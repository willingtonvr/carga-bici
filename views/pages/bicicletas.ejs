<!-- views/pages/bicicletas.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div class="jumbotron">
            <h2>Bicicletas</h2>
            <ul>
              <% bici.forEach(function(bicicleta) { %>
                <li><%= bicicleta.nombre %> - Usos :<%= bicicleta.uses %> </li>
                <% }); %>
            </ul>
        </div>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>

</body>
</html>
